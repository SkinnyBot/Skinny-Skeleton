{
    "name": "skinnybot/skinny-skeleton",
    "description": "A framework to create discord bot in PHP.",
    "homepage": "https://github.com/SkinnyBot/Skinny-Skeleton",
    "keywords": ["discord", "bot", "php", "discordphp", "botphp", "skinny"],
    "type": "project",
    "license": "MIT",
    "support": {
        "source":"https://github.com/SkinnyBot/Skinny-Skeleton",
        "issues":"https://github.com/SkinnyBot/Skinny-Skeleton/issues"
    },
    "authors": [
        {
            "name": "Xety",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": ">=7.3",
        "skinnybot/skinny": "dev-master",
        "skinnybot/plugin-installer": "dev-master",
        "skinnybot/basic": "dev-master",
        "skinnybot/module": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Bot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "Skinny\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "dev"
}
